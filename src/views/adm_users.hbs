<link rel="stylesheet" href="/css/admin/adm_users.css">

<div class="container">
    <h1>Registros de Usuarios</h1>
    <table class="rwd-table">
        <tbody>
            <tr>
                <th>#</th>
                <th>Usuario</th>
                <th>Email</th>
                <th>Telefono</th>
                <th>Contrase√±a</th>
                <th>Acciones</th>
            </tr>
            {{#each users}}
            <tr>
                <td data-th="Supplier Code">
                    {{ id_usuarios }}
                </td>
                <td data-th="Supplier Name">
                    {{ usuario }}
                </td>
                <td data-th="Invoice Number">
                    {{ email }}
                </td>
                <td data-th="Invoice Date">
                    {{ telefono }}
                </td>
                <td data-th="Due Date">
                    {{ contra }}
                </td>
                <td>
                    <a href="/edit" class="btn btn-outline-info">Editar</a>
                    <a href="/delete/" class="btn btn-outline-danger">Borrar</a>
                </td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</div>